    "nodemon": "2.0.4",
    "prismjs": "1.21.0",
    "register-service-worker": "1.7.1",
    "sass-loader": "8.0.2",
    "vue": "2.6.11",
    "vue-eva-icons": "1.1.1",
    "vue-router": "3.2.0",
    "vue-template-compiler": "2.6.11",
    "vuex": "3.4.0",
    "yaml": "1.10.0"
