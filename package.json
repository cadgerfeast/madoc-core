  "license": "MIT",
  "keywords": [
    "madoc",
    "core",
    "madoc-core",
    "markdown",
    "documentation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadgerfeast/madoc-core.git"
  },
  "bin": {
    "madoc": "./bin/cli.js"
  },
  "files": [
    "bin/**/*",
    "public/**/*",
    "src/**/*",
    "babel.config.js",
    "vue.config.js"
  ],
  "scripts": {
