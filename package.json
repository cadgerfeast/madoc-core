    "@vue/cli-plugin-unit-jest": "4.4.0",
    "@vue/cli-plugin-vuex": "4.4.0",
    "@vue/cli-service": "4.4.0",
    "@vue/test-utils": "1.0.3",
    "ansi-colors": "4.1.1",
    "babel-eslint": "10.1.0",
    "chokidar": "3.4.2",
    "core-js": "3.6.5",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "express": "4.17.1",
    "fs-extra": "9.0.1",
    "marked": "1.1.1",
